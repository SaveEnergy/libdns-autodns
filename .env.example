# AutoDNS API Credentials
# Copy this file to .env and fill in your actual credentials

# Your AutoDNS username
AUTODNS_USERNAME=your-username

# Your AutoDNS password
AUTODNS_PASSWORD=your-password

# AutoDNS context (optional, defaults to "4")
# "1" = demo environment, "4" = live environment
AUTODNS_CONTEXT=4

# Your domain for testing (optional)
AUTODNS_ZONE=your-domain.com 